pipeline {
  agent any
    stages {
      stage('Terraform Init') {
        steps {
          sh "terraform init"
        }
      }
      stage('Terraform Plan') {
        steps {
          sh "terraform plan"
        }
      }
    stage('Terraform Dev') {
      steps {
        sh "terraform apply -var="bucketname=multibucket6546" -auto-approve
      }
    }
  } 
}
